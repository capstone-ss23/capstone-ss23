{
    "name": "capstone",
    "version": "0.0.1",
    "description": "Capstone Development Website",
    "private": true,
    "workspaces": [
        "vendor/cl/*",
        "packages/*"
    ],
    "scripts": {
        "build:dev": "webpack --config webpack.dev.js",
        "build:prod": "webpack --config webpack.prod.js",
        "build:all": "$npm_execpath run build:dev && $npm_execpath run build:prod",
        "profile:dev": "webpack --config webpack.dev.js --profile --json > stats.json",
        "profile:prod": "webpack --config webpack.prod.js --json > stats.json",
        "test": "karma start karma.conf.js",
        "serve": "webpack serve --config webpack.serve.js --open",
        "build:docs:course": "cd vendor/cl/course && npm run build:docs",
        "build:docs:site": "cd vendor/cl/site && npm run build:docs",
        "build:docs:interact": "cd vendor/cl/interact && npm run build:docs",
        "build:docs": "npm run build:docs:site && npm run build:docs:course && npm run build:docs:interact",
        "cl-publish": "yarn node vendor/cl/site/publisher.js",
        "build:docs:grades": "cd vendor/cl/grades && npm run build:docs",
        "build:docs:console": "cd vendor/cl/console && npm run build:docs"
    },
    "keywords": [
        "Education",
        "LTI",
        "Course Websites"
    ],
    "author": "Charles B. Owen",
    "license": "MIT",
    "dependencies": {
        "console-cl": "~0.0.1",
        "course-cl": "~0.0.1",
        "grades-cl": "~0.0.1",
        "review-cl": "~0.0.1",
        "site-cl": "^0.0.1",
        "step-cl": "~0.0.1",
        "users-cl": "~0.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.20.7",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.19.6",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.20.2",
        "@babel/runtime": "7.20.7",
        "babel-loader": "^9.1.0",
        "copy-webpack-plugin": "11.0.0",
        "cpx": "^1.5.0",
        "css-loader": "^6.7.3",
        "css-minimizer-webpack-plugin": "^4.2.2",
        "filemanager-webpack-plugin": "^8.0.0",
        "fs-extra": "^11.1.0",
        "make-dir": "^3.1.0",
        "resolve-url-loader": "^5.0.0",
        "rimraf": "^3.0.2",
        "sass": "^1.57.1",
        "sass-loader": "^13.2.0",
        "style-loader": "^3.3.1",
        "terser-webpack-plugin": "^5.3.6",
        "url-loader": "^4.1.1",
        "vue": "^3.2.45",
        "vue-loader": "^17.0.1",
        "vue-template-compiler": "^2.7.14",
        "webpack": "^5.75.0",
        "webpack-chunk-renamer-plugin": "^1.0.1",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "4.11.1",
        "webpack-manifest-plugin": "^5.0.0",
        "webpack-merge": "^5.8.0"
    },
    "packageManager": "yarn@3.3.1"
}
